{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.py"
      }
    ],
    "env": {
      "PYTHONPATH": ".",
      "DJANGO_SETTINGS_MODULE": "Zeno.settings",
      "ALLOWED_HOSTS": ".vercel.app",
      "DEBUG": "False"
    }
}
  